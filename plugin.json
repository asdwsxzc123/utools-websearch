{
  "main": "index.html",
  "preload": "preload.js",
  "name": "websearch",
  "version": "1.0.0",
  "pluginName": "web-search",
  "description": "支持web查询，类似alfred websearch,基于DarmstadtiumGear开发的",
  "logo": "logo.png",

  "development": {
    "main": "http://127.0.0.1:5173/index.html"
  },
  "features": [
    {
      "code": "search-by-default",
      "explain": "搜索指定关键字，name:xxx，匹配name搜索引擎，搜索xxx内容",
      "cmds": [
        {
          "type": "over",
          "label": "搜索",
          "match": "*",
          "minLength": 1,
          "maxLength": 128
        }
      ]
    },
    {
      "code": "search-tip",
      "explain": "搜索提示",
      "mainPush":false,
      "cmds": [
        "tip"
      ]
    },
    {
      "code": "search-config",
      "explain": "配置搜索引擎列表",
      "cmds": [
        "config"
      ]
    }
  ]

}
